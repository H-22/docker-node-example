pipeline {
  agent any
  stages {
    stage('Test') {
      steps {
        echo 'Démarrage du stage Test'
        sleep 10
        echo 'Fin du stage Test'
      }
    }

    stage('Build') {
      steps {
        echo 'Démarrage du stage Build'
        sleep 10
        echo 'Fin du stage Build'
      }
    }

    stage('Deploy') {
      steps {
        echo 'Démarrage du stage Deploy'
        sleep 10
        echo 'Fin du stage Deploy'
      }
    }

  }
}